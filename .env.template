# Biowell AI Environment Variables Template
# Copy this file to .env and fill in your actual values

# Required - Supabase Configuration
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Optional - AI Services (for enhanced features)
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Optional - External APIs
VITE_RAPIDAPI_KEY=your-rapidapi-key
VITE_SPOONACULAR_API_KEY=your-spoonacular-api-key

# Optional - Analytics & Monitoring
VITE_ANALYTICS_ID=your-analytics-id

# Development Mode
# Set to 'true' to enable demo/mock data for development and testing.
# Set to 'false' in production to use real data and live features.
# Features affected: When 'true', the app may use mock data, disable some external API calls, or enable test-only features.
VITE_DEMO_MODE=false
