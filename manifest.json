{
  "name": "Biowell AI - Personal Health Coach",
  "short_name": "Biowell AI",
  "description": "Your AI-powered personal health coach for nutrition, fitness, and wellness guidance",
  "theme_color": "#0c6acd",
  "background_color": "#ffffff",
  "display": "standalone",
  "orientation": "portrait-primary",
  "scope": "/",
  "start_url": "/",
  "id": "/",
  "lang": "en-US",
  "dir": "ltr",
  "categories": ["health", "lifestyle", "medical", "fitness", "nutrition"],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 480
  },
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "protocol_handlers": [
    {
      "protocol": "web+biowell",
      "url": "/health-data?data=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import-health-data",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/share-health-data",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "health_data",
          "accept": ["text/csv", "application/json", "image/*"]
        }
      ]
    }
  },
  "shortcuts": [
    {
      "name": "AI Health Coach",
      "short_name": "Chat",
      "description": "Chat with your AI health coach",
      "url": "/coach",
      "icons": [
        {
          "src": "icons/shortcut-chat.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Health Dashboard",
      "short_name": "Dashboard",
      "description": "View your health metrics and progress",
      "url": "/dashboard",
