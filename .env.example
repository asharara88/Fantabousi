# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# JWT Secret for authentication
JWS_SECRET=your_jwt_secret_here

# OpenAI API for AI coaching features
OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs API for voice synthesis
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# RapidAPI for muscle image analysis
VITE_RAPIDAPI_MUSCLE_IMAGE_KEY=your_rapidapi_key_here

# Spoonacular API for nutrition data
VITE_SPOONACULAR_API_KEY=your_spoonacular_api_key_here

# Nutritionix API for food database
VITE_NUTRITIONIX_API_KEY=your_nutritionix_api_key_here
VITE_NUTRITIONIX_APP_ID=your_nutritionix_app_id_here

# Google OAuth for authentication
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
