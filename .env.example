# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# JWT Secret for authentication
JWS_SECRET=your_jwt_secret_here

# OpenAI API for AI coaching features
OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs API for voice synthesis
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# RapidAPI for muscle image analysis
VITE_RAPIDAPI_MUSCLE_IMAGE_KEY=your_rapidapi_key_here

# Spoonacular API for nutrition data
VITE_SPOONACULAR_API_KEY=your_spoonacular_api_key_here

# Nutritionix API for food database
VITE_NUTRITIONIX_API_KEY=your_nutritionix_api_key_here
VITE_NUTRITIONIX_APP_ID=your_nutritionix_app_id_here

# Google OAuth for authentication
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Stripe Configuration for Payments
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key_here
# For development/testing, use:
# VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_key_here

# Netlify Build Configuration
NODE_ENV=production
CI=true
GENERATE_SOURCEMAP=false

# Environment Identifier
VITE_ENV=production

# API Base URL (if using external backend)
VITE_API_BASE_URL=https://api.biowell.ai/v1

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT=true
VITE_ENABLE_PAYMENTS=true

# Analytics (Optional)
VITE_GA_TRACKING_ID=your_google_analytics_id_here
