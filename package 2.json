{
  "name": "biowell-digital-coach-lite",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --host localhost",
    "dev:setup": "chmod +x dev-start.sh && ./dev-start.sh",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "lint": "eslint . --max-warnings 50",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "lint:strict": "eslint . --max-warnings 0",
    "preview": "vite preview --port 5173",
    "clean": "rm -rf dist node_modules/.vite .next && npm install",
    "clean:cache": "rm -rf node_modules/.vite",
    "replace-console-error": "node --loader ts-node/esm scripts/replaceConsoleErrorAndImport.ts",
    "fetch:feeds": "node --loader ts-node/esm scripts/fetchFeeds.ts",
    "test": "npm run build",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "postinstall": "echo 'Skipping problematic native builds'",
    "predeploy": "npm run build:prod",
    "deploy": "netlify deploy --prod",
    "start": "./dev-start.sh",
    "env:check": "node -e \"console.log('Environment check:', process.env)\""
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.52.1",
    "@types/three": "^0.178.1",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "framer-motion": "^12.23.10",
    "lucide-react": "^0.528.0",
    "openai": "^5.10.2",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.21.1",
    "react-select": "^5.10.2",
    "recharts": "^3.1.0",
    "supabase": "^2.31.8",
    "tailwind-merge": "^3.3.1",
    "three": "^0.169.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.9.0",
    "jsdom": "^24.1.3",
    "msw": "^2.8.4",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "xml2js": "^0.6.2",
    "node-expat": "npm:@xml-tools/parser@^1.0.11"
  },
  "npm": {
    "prebuild": false
  }
}
